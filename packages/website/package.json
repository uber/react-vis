{
  "name": "react-vis-website",
  "private": true,
  "version": "1.0.0",
  "description": "A composable charting library",
  "main": "gatsby-config.js",
  "scripts": {
    "clean": "rm -rf dist/*{.js,.css,index.html,appcache,fonts,images} && npm run clean-ocular",
    "clean-ocular": "rm -rf ./.cache ./public",
    "develop": "gatsby develop --port=8000",
    "build": "npm run clean && npm run build-ocular && npm run build-storybook",
    "build-storybook": "build-storybook -c .storybook -o ./dist/storybook",
    "build-ocular": "gatsby build",
    "lint": "ocular lint",
    "serve": "gatsby serve",
    "start": "yarn clean && yarn develop",
    "storybook": "start-storybook -p 9001 -c .storybook"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@storybook/addon-knobs": "^5.3.18",
    "@storybook/react": "^5.3.18",
    "gatsby": "~2.20.24",
    "ocular-gatsby": "^1.0.6",
    "storybook-addon-jsx": "^7.2.3",
    "to-case": "^2.0.0"
  },
  "dependencies": {
    "d3-force": "^1.0.6",
    "d3-random": "^1.1.0",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-virtualized": "^9.18.5",
    "react-vis": "^1.11.2"
  },
  "volta": {
    "node": "10.20.1",
    "yarn": "1.22.4"
  }
}
