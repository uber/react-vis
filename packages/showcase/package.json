{
  "name": "react-vis-showcase",
  "description": "Showcase of react-vis components",
  "version": "0.1.0",
  "scripts": {
    "start": "webpack-dev-server --progress --hot --port 3001",
    "build": "NODE_ENV=production webpack",
    "build:windows": "set NODE_ENV=production&&.\\node_modules\\.bin\\webpack"
  },
  "devDependencies": {
    "babel-loader": "^7",
    "css-loader": "^0.26.1",
    "mini-css-extract-plugin": "^0.9.0",
    "sass-loader": "^8.0.2",
    "style-loader": "^0.13.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "license": "MIT",
  "private": true,
  "dependencies": {
    "d3-force": "^1.0.6",
    "d3-random": "^1.1.0",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-vis": "^1.11.7"
  },
  "volta": {
    "node": "10.20.1",
    "yarn": "1.22.4"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "react"
    ],
    "plugins": [
      [
        "module-resolver",
        {
          "root": [
            "."
          ],
          "alias": {
            "react-vis/*": "../react-vis/src/*",
            "react-vis": "../react-vis/src"
          }
        }
      ]
    ]
  }
}
